<h1>Edit <%= @store.name %></h1>

<form action="/stores/<%=@store.id %>" method="POST">
  <label> Edit Store Name </label>
    <input type="text" name="name" value="<%=@store.name %>"> </br>

  <label> Edit Store Category </label>
    <input type="text" name="category" value="<%=@store.category%>"> </br>

  <input type="submit" value="Update">
</form>
